{
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "workbench.colorTheme": "Material Theme",
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "boundary",
    "workbench.statusBar.visible": true,
    "extensions.ignoreRecommendations": false,
    "explorer.confirmDelete": false,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#80CBC4",
        "list.activeSelectionForeground": "#80CBC4",
        "list.inactiveSelectionForeground": "#80CBC4",
        "list.highlightForeground": "#80CBC4",
        "scrollbarSlider.activeBackground": "#80CBC450",
        "editorSuggestWidget.highlightForeground": "#80CBC4",
        "textLink.foreground": "#80CBC4",
        "progressBar.background": "#80CBC4",
        "pickerGroup.foreground": "#80CBC4",
        "tab.activeBorder": "#80CBC4",
        "notificationLink.foreground": "#80CBC4",
        "editorWidget.resizeBorder": "#80CBC4",
        "editorWidget.border": "#80CBC4",
        "settings.modifiedItemForeground": "#80CBC4",
        "panelTitle.activeBorder": "#80CBC4"
    },
    "html.format.endWithNewline": true,
    "html.format.enable": false,
    "materialTheme.accent": "Teal",
    "editor.cursorBlinking": "phase",
    "breadcrumbs.enabled": true,
    "workbench.editor.closeOnFileDelete": true,
    "window.openFoldersInNewWindow": "on",
    "search.showLineNumbers": true,
    "editor.largeFileOptimizations": false,
    "editor.fontSize": 16,
    "editor.fontWeight": "500",
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "rename": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true,
        "goToImplementation": true,
        "documentSymbols": true,
        "workspaceSymbols": true,
        "findReferences": true,
        "diagnostics": true,
        "documentLink": true
    },
    "go.useLanguageServer": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "terraform.languageServer": {},
    "npm.fetchOnlinePackageInfo": false,
    "json.schemaDownload.enable": false,
    "diffEditor.renderSideBySide": false,
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "workbench.editor.wrapTabs": true,
    "twig-language-2.indentStyle": "space",
    "twig-language-2.quoteConvert": "double",
    "editor.detectIndentation": false,
    "editor.rulers": [
        80, 120
    ],
    "todohighlight.keywords": [
     "kludge:",
     "warn:",
     {
            "text": "TODO:",
            "color": "white",
            "backgroundColor": "crimson"
        }
    ],
    "todohighlight.isCaseSensitive": false,
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/vendor/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**"
    ],
    "todohighlight.toggleURI": true,
    "codesnap.backgroundColor": "",
    "window.zoomLevel": 1,
    "codesnap.transparentBackground": true,
    "thunder-client.codeSnippetLanguage": "curl",
    "intelephense.format.enable": false,
    "[php]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "junstyle.php-cs-fixer"
    },
    "php-cs-fixer.lastDownload": 0,
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "php-cs-fixer.autoFixByBracket": false,
    "twig-language-2.formatting": false,
    "security.workspace.trust.untrustedFiles": "open",
}