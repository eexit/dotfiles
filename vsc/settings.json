{
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "boundary",
    "workbench.statusBar.visible": true,
    "extensions.ignoreRecommendations": true,
    "explorer.confirmDelete": false,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#80CBC4",
        "list.activeSelectionForeground": "#80CBC4",
        "list.inactiveSelectionForeground": "#80CBC4",
        "list.highlightForeground": "#80CBC4",
        "scrollbarSlider.activeBackground": "#80CBC450",
        "editorSuggestWidget.highlightForeground": "#80CBC4",
        "textLink.foreground": "#80CBC4",
        "progressBar.background": "#80CBC4",
        "pickerGroup.foreground": "#80CBC4",
        "tab.activeBorder": "#80CBC4",
        "notificationLink.foreground": "#80CBC4",
        "editorWidget.resizeBorder": "#80CBC4",
        "editorWidget.border": "#80CBC4",
        "settings.modifiedItemForeground": "#80CBC4",
        "panelTitle.activeBorder": "#80CBC4",
        "editorBracketHighlight.foreground1": "#82aaff",
        "editorBracketHighlight.foreground2": "#82aaff",
        "editorBracketHighlight.foreground3": "#82aaff",
        "editorBracketHighlight.foreground4": "#82aaff",
        "editorBracketHighlight.foreground5": "#82aaff",
        "editorBracketHighlight.foreground6": "#82aaff",
    },
    "workbench.editor.closeOnFileDelete": true,
    "window.openFoldersInNewWindow": "on",
    "search.showLineNumbers": true,
    "editor.largeFileOptimizations": false,
    "editor.fontSize": 16,
    "editor.fontWeight": "500",
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "rename": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true,
        "goToImplementation": true,
        "documentSymbols": true,
        "workspaceSymbols": true,
        "findReferences": true,
        "diagnostics": true,
        "documentLink": true
    },
    "go.useLanguageServer": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "npm.fetchOnlinePackageInfo": false,
    "json.schemaDownload.enable": false,
    "workbench.editor.wrapTabs": true,
    "twig-language-2.indentStyle": "space",
    "twig-language-2.quoteConvert": "double",
    "editor.detectIndentation": false,
    "editor.rulers": [
        80,
        120
    ],
    "todohighlight.keywords": [
        "kludge:",
        "warn:",
        {
            "text": "TODO:",
            "color": "white",
            "backgroundColor": "crimson"
        }
    ],
    "todohighlight.isCaseSensitive": false,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.twig"
    ],
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/vendor/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**"
    ],
    "todohighlight.toggleURI": true,
    "codesnap.backgroundColor": "",
    "codesnap.transparentBackground": true,
    "twig-language-2.formatting": false,
    "security.workspace.trust.untrustedFiles": "open",
    "todohighlight.isEnable": true,
    "intelephense.environment.phpVersion": "8.1.0",
    "editor.hover.delay": 600,
    "references.preferredLocation": "view",
    "keyboard.touchbar.enabled": false,
    "update.showReleaseNotes": false,
    "window.newWindowDimensions": "inherit",
    "window.openFilesInNewWindow": "on",
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.useSplitJSON": true,
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "phase",
    "editor.minimap.showSlider": "always",
    "editor.minimap.scale": 2,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "xmlTools.splitXmlnsOnFormat": false,
    "materialTheme.accent": "Teal",
    "workbench.colorTheme": "Material Theme",
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.highlightActiveBracketPair": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "phpstan.suppressTimeoutMessage": true,
    "files.associations": {
        "*.twig": "twig",
        "*.neon": "yaml"
    },
    "colorize.languages": [
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg",
        "php",
        "twig",
    ],
    "colorize.enable_search_variables": false,
    "colorize.colorized_colors": [
        "HEXA",
        "RGB",
        "HSL"
    ],
    "html.format.wrapLineLength": 0,
    "html.format.templating": true,
    "html.format.indentHandlebars": true,
    "editor.formatOnSave": true,
    "html.format.contentUnformatted": "",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "html.format.extraLiners": "",
    "go.toolsManagement.autoUpdate": true,
    "redhat.telemetry.enabled": false,
    "yaml.format.singleQuote": true,
    "yaml.format.enable": false,
    "debug.console.fontSize": 16,
    "window.zoomLevel": 1,
    "tabnine.experimentalAutoImports": true,
    "php.validate.executablePath": "/opt/homebrew/bin/php",
    "intelephense.trace.server": "messages",
    "intelephense.format.enable": false,
    "php-cs-fixer.lastDownload": 0,
    "[php]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "junstyle.php-cs-fixer"
    },
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.autoFixByBracket": false
}
